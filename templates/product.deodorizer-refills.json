    "2588bd67-a335-4aaa-bff7-516eeffc77b4": {
      "type": "cust-faq",
      "blocks": {
        "item_Pyhe64": {
          "type": "item",
          "settings": {
            "title": "Do you ship globally?",
            "content": "<p>Yes, we ship to most countries in the world. Shipping costs will apply, and will be added at checkout. We run discounts and promotions all year, so stay tuned for exclusive deals.</p>"
          }
        },
        "item_kcEcrn": {
          "type": "item",
          "settings": {
            "title": "What is your return policy?",
            "content": "<p>All products purchased from our website may be returned or replaced within 30 days of purchase for a full refund less shipping costs, no questions asked. International return orders may be subject to return shipping fees. To initiate such a return please <a href=\"/pages/contact-us\" target=\"_blank\" title=\"Contact Us\">contact us</a>.</p>"
          }
        },
        "item_cezCEM": {
          "type": "item",
          "settings": {
            "title": "How long will it take to get my orders?",
            "content": "<p>It depends on where you are. Orders processed here will take 5-7 business days to arrive. Overseas deliveries can take anywhere from 7-16 days. Delivery details will be provided in your confirmation email.</p>"
          }
        },
        "item_E6kRBb": {
          "type": "item",
          "settings": {
            "title": "Where can I download the user manual?",
            "content": "<p>Write content to answer to common questions your customers may have about your products, shipping policies..</p>{{ product.metafields.custom.user_manual | metafield_tag }}"
          }
        },
        "item_8TckTg": {
          "type": "item",
          "settings": {
            "title": "Any other questions?",
            "content": "<p>You can contact us through our <a href=\"/pages/contact-us\" target=\"_blank\" title=\"Contact Us\">contact page</a>! We will be happy to assist you.</p>"
          }
        }
      },
