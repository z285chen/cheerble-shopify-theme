 height: 70px; /*  */
 background: #000000 !important;
 z-index: 2147483647 !important;
 
 opacity: 0;
 visibility: hidden;
 transform: translateY(-100%);
 transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
}
/*  70px */
.innerNav {
 height: 100%;
 width: 100%;
}
.g1-sub-nav-fixed.is-active {
 opacity: 1;
 visibility: visible;
 transform: translateY(0);
}
/* 2.  */
.innerNav-content {
 max-width: 1200px;
 margin: 0 auto;
 height: 100%; /*  innerNav  100%  */
 display: flex;
 align-items: center; /*  */
 justify-content: flex-start; /*  */
 gap: 40px; /*  */
 padding: 0 20px;
 box-sizing: border-box;
}
/*  */
.innerNav-title { 
 color: #fff; 
 font-weight: bold; 
 font-size: 18px; 
 margin: 0 !important; /*  magin -> margin */
 display: flex; 
 align-items: center;
 height: 100%; 
 white-space: nowrap; /*  */
}
/*  */
.innerNav-catalog { 
 display: flex; 
 gap: 20px; 
 height: 100%;
 align-items: center; /*  flex  */
}
/*  */
.innerNav-catalog-item { 
 color: #999; 
 cursor: pointer; 
 font-size: 14px; 
 font-weight: 500; 
 height: 100%; 
 display: flex; 
 align-items: center; /*  */
