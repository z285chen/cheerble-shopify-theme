 }
}
</style>
<script>
(function() {
 function initSubNav() {
 const subNav = document.getElementById('G1-Custom-SubNav');
 if (!subNav) return;
 //  body 
 //  EComposer Section 
 document.body.appendChild(subNav);
 const triggerSection = document.getElementById('g1-hero-trigger');
 const themeHeader = document.querySelector('.header-wrapper, #shopify-section-header, .header');
 window.addEventListener('scroll', function() {
 if (!triggerSection) return;
 //  Hero 
 const triggerBottom = triggerSection.getBoundingClientRect().bottom;
 // 
 if (triggerBottom <= 0) { 
 subNav.classList.add('is-active');
 if (themeHeader) themeHeader.style.zIndex = "1"; // 
 } else {
 subNav.classList.remove('is-active');
 if (themeHeader) themeHeader.style.zIndex = ""; // 
 }
 });
 // 
 document.querySelectorAll('.innerNav-catalog-item').forEach(item => {
 item.addEventListener('click', function() {
 const targetId = this.getAttribute('data-target');
 const targetEl = document.getElementById(targetId);
 if (targetEl) {
 const offset = 60;
 const targetPosition = targetEl.getBoundingClientRect().top + window.pageYOffset - offset;
 window.scrollTo({ top: targetPosition, behavior: 'smooth' });
 }
 });
 });
 }
 // 
 if (document.readyState === 'complete') {
 initSubNav();
 } else {
 window.addEventListener('load', initSubNav);
 }
})();
